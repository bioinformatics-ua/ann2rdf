{
  "namespace":"http://bioinformatics.ua.pt/slito/",
  "prefix":"slito",
  "normalization":[
    {
      "service":"http://bioinformatics.ua.pt/becas/api/text/annotate?email=sernadela@ua.pt&tool=test",
      "query":"*.*.refs",
      "enable":true
    },
    {
      "service":"http://data.bioontology.org/annotator?apikey=b3fe8ec4-42f9-4b13-9007-417ddee3caf9",
      "query":"[*].annotatedClass.@id",
      "enable":false
    }
  ],
  "mappings":
    {
      "tags":[
        {
          "if":"Cell",
          "then": "http://purl.bioontology.org/ontology/SNOMEDCT/4421005"
        },
        {
          "if":"Gene_or_gene_product",
          "then":"Gene"
        }
      ]
    ,
      "relations":[
        {
          "if":"trigger",
          "then": "Trigger"
        },
        {
          "if":"Arg1",
          "then":"http://www.bootstrep.eu/ontology/GRO#partOf"
        },
        {
          "if":"Arg2",
          "then":"http://www.bootstrep.eu/ontology/GRO#partOf"
        }
      ]
    }
}